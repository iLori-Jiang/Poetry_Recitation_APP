         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="当前具有如下Tag：" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Text>
         </top>
         <center>
            <Text fx:id="Tags" fill="#45c8d3" strokeType="OUTSIDE" strokeWidth="0.0" text="Tags" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <BorderPane.margin>
                  <Insets top="5.0" />
               </BorderPane.margin>
            </Text>
         </center>
         <padding>
